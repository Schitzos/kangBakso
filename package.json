{
  "name": "kangbakso",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "start": "react-native start",
    "start-clear": "npm start -- --reset-cache",
    "test": "jest",
    "test:cover": "jest --collectCoverage --coverageDirectory=\"./coverage\" && open ./coverage/index.html",
    "android:clean": "cd android && ./gradlew clean",
    "android:build-debug-apk": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "android:generate-google-services-json": "node src/utils/scripts/generateGoogleServicesJson.js",
    "ios:pod-install": "cd ios && pod install --verbose",
    "ios:generate-google-services-plist": "node src/utils/scripts/generateGoogleServicesInfoPlist.js",
    "kill": "watchman watch-del-all",
    "postinstall": "npm run android:generate-google-services-json && if [[ $(uname) == 'Darwin' ]]; then npm run ios:pod-install && npm run ios:generate-google-services-plist; fi"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.5",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^21.3.0",
    "@react-native-firebase/auth": "^21.3.0",
    "@react-native-firebase/crashlytics": "^21.3.0",
    "@react-native-firebase/firestore": "^21.3.0",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.60.5",
    "base-64": "^1.0.0",
    "react": "18.3.1",
    "react-hook-form": "^7.53.1",
    "react-native": "0.75.4",
    "react-native-background-actions": "^4.0.1",
    "react-native-bootsplash": "^6.2.5",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-exit-app": "^2.0.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-maps": "^1.18.2",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^3.35.0",
    "react-native-svg": "^15.8.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-toast-message": "^2.2.1",
    "yup": "^1.4.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-react-native": "^18.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "2.8.8",
    "react-native-config": "^1.5.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-reanimated": "^3.16.2",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --fix"
    ]
  }
}
